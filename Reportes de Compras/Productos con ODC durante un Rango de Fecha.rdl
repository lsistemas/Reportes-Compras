    <DataSource Name="VAD10_Prod">
      <DataSourceReference>VAD10_Prod</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>4bb29901-c344-46c5-b5b1-6b090d0e42c7</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>VAD10_Prod</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FechaDesde">
            <Value>=Parameters!FechaDesde.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FechaHasta">
            <Value>=Parameters!FechaHasta.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Sucursal">
            <Value>=Parameters!Sucursal.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT D.C_DESCRIPCIO AS DEPARTAMENTO, P.C_CODIGO AS CODIGO_PRODUCTO, P.C_DESCRI AS PRODUCTO, 1 AS SKU
FROM         TR_ODC AS DOC INNER JOIN
                      MA_ODC AS COC ON DOC.c_DOCUMENTO = COC.c_DOCUMENTO INNER JOIN
                      MA_PRODUCTOS AS P ON DOC.c_CODARTICULO = P.C_CODIGO INNER JOIN
                      MA_DEPARTAMENTOS AS D ON P.c_departamento = D.C_CODIGO
WHERE     (CONVERT(DATE, COC.d_FECHA) BETWEEN @FechaDesde AND @FechaHasta) AND COC.C_CODLOCALIDAD= @Sucursal</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DEPARTAMENTO">
          <DataField>DEPARTAMENTO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CODIGO_PRODUCTO">
          <DataField>CODIGO_PRODUCTO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRODUCTO">
          <DataField>PRODUCTO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SKU">
          <DataField>SKU</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>VAD10_Prod</DataSourceName>
        <CommandText>SELECT     C_codigo AS CODIGO_SUCURSAL, c_descripcion AS SUCURSAL
FROM         MA_SUCURSALES</CommandText>
      </Query>
      <Fields>
        <Field Name="CODIGO_SUCURSAL">
          <DataField>CODIGO_SUCURSAL</DataField>
