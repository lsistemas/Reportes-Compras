  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>VAD10_Prod</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FechaDesde">
            <Value>=Parameters!FechaDesde.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FechaHasta">
            <Value>=Parameters!FechaHasta.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT month(HP.fecha) as mes, HP.c_codigo AS CODIGO_PRODUCTO, P.C_DESCRI AS PRODUCTO, D.C_DESCRIPCIO AS DEPARTAMENTO
--, month(hpa.fecha) as mes_anterior
--,hpa.Activo as estado_anterio
--,hp.Activo as estado_actual
, CASE WHEN SUM(CASE WHEN HPA.activo=1 AND HP.Activo=0 then 1 else 0 end)&gt;0 THEN 1 ELSE 0 END as CANTIDAD_INACTIVO


  FROM [ExtendVad10].[dbo].[HistoricoCostoPrecio] as HP
  INNER JOIN VAD10.DBO.MA_PRODUCTOS AS P ON HP.c_codigo=P.C_CODIGO
  LEFT OUTER JOIN VAD10.DBO.MA_DEPARTAMENTOS AS D ON P.c_departamento=D.C_CODIGO
  INNER JOIN (
  SELECT hp1.fecha, hp1.c_codigo, hp1.Activo
FROM ExtendVad10.DBO.HistoricoCostoPrecio AS HP1
WHERE hp1.Fecha between DATEADD(DAY,-1,@FechaDesde) and DATEADD(DAY,-1,@FechaHasta))AS HPA ON
HP.c_codigo=HPA.c_codigo and HP.Fecha=DATEADD(DAY,1,HPA.FECHA)
  where HP.Fecha between @FechaDesde and @FechaHasta and CAST(P.add_date AS DATE)&lt; @FechaDesde 
  group by month(HP.fecha) , HP.c_codigo, P.C_DESCRI , D.C_DESCRIPCIO</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="mes">
          <DataField>mes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DEPARTAMENTO">
          <DataField>DEPARTAMENTO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CODIGO_PRODUCTO">
