          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRODUCTO">
          <DataField>PRODUCTO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CANTIDAD_INACTIVO">
          <DataField>CANTIDAD_INACTIVO</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
