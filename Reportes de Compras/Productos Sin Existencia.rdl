  <DataSets>
    <DataSet Name="DataSet_SinStock">
      <Query>
        <DataSourceName>VAD10</DataSourceName>
        <CommandText>SELECT     Q.CODIGO_PRODUCTO, Q.PRODUCTO, Q.CODIGO_DPTO, Q.DPTO, Q.CODIGO_SUCURSAL, Q.SUCURSAL, Q.COD_GRUPO, Q.GRUPO, Q.COD_SUBGRUPO, 
                      Q.SUBGRUPO, Q.STOCK, CAST(MAX(R.f_fecha) AS DATE) AS FECHA_ULT_REC, CAST(MAX(ODC.d_FECHA) AS DATE) AS FECHA_ULT_ODC, CAST(MAX(V.FECHA) 
                      AS DATE) AS FECHA_ULT_VEN
FROM         (SELECT     p.C_CODIGO AS CODIGO_PRODUCTO, p.C_DESCRI AS PRODUCTO, DPTO.C_CODIGO AS CODIGO_DPTO, DPTO.C_DESCRIPCIO AS DPTO, 
                                              G.c_CODIGO AS COD_GRUPO, G.C_DESCRIPCIO AS GRUPO, S1.c_CODIGO AS COD_SUBGRUPO, S1.c_DESCRIPCIO AS SUBGRUPO, 
                                              S.C_codigo AS CODIGO_SUCURSAL, S.c_descripcion AS SUCURSAL, ROUND(SUM(E.n_cantidad), 4) AS STOCK
                       FROM          MA_PRODUCTOS AS p INNER JOIN
                                              MA_DEPARTAMENTOS AS DPTO ON p.c_departamento = DPTO.C_CODIGO INNER JOIN
                                              MA_DEPOPROD AS E ON p.C_CODIGO = E.c_codarticulo INNER JOIN
                                              MA_DEPOSITO AS D ON D.c_coddeposito = E.c_coddeposito INNER JOIN
                                              MA_SUCURSALES AS S ON D.c_codlocalidad = S.C_codigo LEFT OUTER JOIN
                                              MA_GRUPOS AS G ON p.c_departamento = G.c_departamento AND p.c_grupo = G.c_CODIGO LEFT OUTER JOIN
                                              MA_SUBGRUPOS AS S1 ON p.c_departamento = S1.c_in_departamento AND p.c_grupo = S1.c_in_grupo AND p.c_subgrupo = S1.c_CODIGO
                       WHERE      (p.n_activo = 1)
                       GROUP BY p.C_CODIGO, p.C_DESCRI, DPTO.C_CODIGO, DPTO.C_DESCRIPCIO, S.C_codigo, S.c_descripcion, G.c_CODIGO, G.C_DESCRIPCIO, S1.c_CODIGO, 
                                              S1.c_DESCRIPCIO) AS Q LEFT OUTER JOIN
                      TR_INVENTARIO AS R ON Q.CODIGO_SUCURSAL = R.c_codlocalidad AND Q.CODIGO_PRODUCTO = R.c_CODARTICULO AND R.c_CONCEPTO = 'REC' AND 
                      R.c_DOCUMENTO NOT LIKE 'A%' LEFT OUTER JOIN
                          (SELECT     O.c_DOCUMENTO, O.c_CODLOCALIDAD, O.d_FECHA, DO.c_CODARTICULO
                            FROM          MA_ODC AS O INNER JOIN
                                                   TR_ODC AS DO ON O.c_DOCUMENTO = DO.c_DOCUMENTO
                            WHERE      (O.c_status &lt;&gt; 'ANU') AND (O.c_DOCUMENTO NOT LIKE 'A%')) AS ODC ON Q.CODIGO_SUCURSAL = ODC.c_CODLOCALIDAD AND 
                      Q.CODIGO_PRODUCTO = ODC.c_CODARTICULO LEFT OUTER JOIN
                          (SELECT     c_CODARTICULO, CAST(MAX(f_fecha) AS DATE) AS FECHA
                            FROM          TR_INVENTARIO AS TI
                            WHERE      (c_DOCUMENTO NOT LIKE 'A%') AND (c_CONCEPTO = 'VEN')
                            GROUP BY c_CODARTICULO) AS V ON Q.CODIGO_PRODUCTO = V.c_CODARTICULO
WHERE     (Q.STOCK = 0)
GROUP BY Q.CODIGO_PRODUCTO, Q.PRODUCTO, Q.CODIGO_DPTO, Q.DPTO, Q.CODIGO_SUCURSAL, Q.SUCURSAL, Q.COD_GRUPO, Q.GRUPO, Q.COD_SUBGRUPO, 
                      Q.SUBGRUPO, Q.STOCK</CommandText>
      </Query>
