    <DataSet Name="DataSet_Compras">
      <Query>
        <DataSourceName>ExtendVAD10</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Ano">
            <Value>=Parameters!Ano.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Sucursal">
            <Value>=Parameters!Sucursal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Departamento">
            <Value>=Parameters!Departamento.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        MONTH(H.fecha) AS MES, H.c_CODARTICULO AS c_codigo, P.c_departamento AS codigo_dpto, SUM(ISNULL(H.cant_COMP, 0)) AS Cantidad_Recibidas, SUM(ISNULL(H.valor_com, 0)) AS Costo_Recepcion, 
                         SUM(ISNULL(H.cant_dev_COMP, 0)) AS Cant_Dev_Compras, SUM(ISNULL(H.valor_DEV_com, 0)) AS Costo_Dev_Compras, SUM(ISNULL(H.cant_PROD, 0)) AS Cantidad_Producidas, SUM(ISNULL(H.valor_PROD, 0)) 
                         AS Costo_Produccion
FROM            HistoricoDeCompras AS H INNER JOIN
                         VAD10.dbo.MA_PRODUCTOS AS P ON H.c_CODARTICULO = P.C_CODIGO
WHERE        (H.fecha BETWEEN CAST(CAST(@Ano AS varchar(4)) + '0101' AS DATE) AND CAST(CAST(@Ano AS varchar(4)) + '1231' AS DATE)) AND (H.Sucursal = @Sucursal) AND (H.c_CODARTICULO &lt;&gt; ' ') AND 
                         (P.c_departamento IN (@Departamento))
GROUP BY H.fecha, H.c_CODARTICULO, P.c_departamento</CommandText>
      </Query>
      <Fields>
        <Field Name="MES">
          <DataField>MES</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="c_codigo">
