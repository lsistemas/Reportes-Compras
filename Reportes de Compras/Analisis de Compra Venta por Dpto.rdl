    <DataSet Name="DataSet_IndiceCompra">
      <Query>
        <DataSourceName>VAD10</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Ano">
            <Value>=Parameters!Ano.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Sucursal">
            <Value>=Parameters!Sucursal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Activo">
            <Value>=Parameters!Activo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Departamento">
            <Value>=Parameters!Departamento.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Mes, c_departamento AS codigo_dpto, C_DESCRIPCIO AS departamento, SUM(CostoVenta) AS TotalCostoVenta, SUM(UTILIDAD) AS UTILIDAD, SUM(TotalVentasL) AS TotalVentasL, SUM(unidadesVenta) 
                         AS unidadesVenta, SUM(TOTALCompra) AS TOTALCompra, SUM(UnidadesCompra) AS UnidadesCompra, CASE SUM(CostoVenta) WHEN 0 THEN 0 ELSE SUM(TOTALCompra) / SUM(CostoVenta) 
                         END AS IndiceComVen, CASE SUM(TotalVentasL) WHEN 0 THEN 0 ELSE SUM(UTILIDAD) / SUM(TotalVentasL) * 100 END AS PorUtilidad
FROM            (SELECT        MONTH(TR_INVENTARIO.f_fecha) AS Mes, MA_PRODUCTOS.c_departamento, MA_DEPARTAMENTOS.C_DESCRIPCIO, SUM((TR_INVENTARIO.n_COSTO * TR_INVENTARIO.n_CANTIDAD) 
                                                    * (MA_VENTAS.n_FACTORCAMBIO / MA_MONEDAS.n_factor)) AS CostoVenta, SUM(((TR_INVENTARIO.n_TOTAL - TR_INVENTARIO.n_IMPUESTO) 
                                                    - TR_INVENTARIO.n_COSTO * TR_INVENTARIO.n_CANTIDAD) * (MA_VENTAS.n_FACTORCAMBIO / MA_MONEDAS.n_factor)) AS UTILIDAD, 
                                                    SUM((TR_INVENTARIO.n_TOTAL - TR_INVENTARIO.n_IMPUESTO) * (MA_VENTAS.n_FACTORCAMBIO / MA_MONEDAS.n_factor)) AS TotalVentasL, SUM(TR_INVENTARIO.n_CANTIDAD) 
                                                    AS unidadesVenta, 0 AS TOTALCompra, 0 AS UnidadesCompra
                          FROM            TR_INVENTARIO AS TR_INVENTARIO LEFT OUTER JOIN
                                                    MA_VENTAS AS MA_VENTAS ON MA_VENTAS.c_DOCUMENTO = TR_INVENTARIO.c_DOCUMENTO AND MA_VENTAS.c_CONCEPTO = TR_INVENTARIO.c_CONCEPTO LEFT OUTER JOIN
                                                    MA_MONEDAS AS MA_MONEDAS ON MA_MONEDAS.c_codmoneda = '0000000002' LEFT OUTER JOIN
                                                    MA_PRODUCTOS ON MA_PRODUCTOS.C_CODIGO = TR_INVENTARIO.c_CODARTICULO LEFT OUTER JOIN
                                                    MA_DEPARTAMENTOS ON MA_PRODUCTOS.c_departamento = MA_DEPARTAMENTOS.C_CODIGO
                          WHERE        (TR_INVENTARIO.c_CONCEPTO = 'VEN') AND (TR_INVENTARIO.n_SUBTOTAL &gt;= 0) AND (TR_INVENTARIO.f_fecha BETWEEN CAST(CAST(@Ano AS varchar(4)) + '0101' AS DATE) AND 
                                                    CAST(CAST(@Ano AS varchar(4)) + '1231' AS DATE)) AND (TR_INVENTARIO.c_codlocalidad = @Sucursal) AND (MA_PRODUCTOS.n_activo IN (@Activo)) AND 
                                                    (MA_PRODUCTOS.c_departamento IN (@Departamento))
                          GROUP BY MONTH(TR_INVENTARIO.f_fecha), MA_PRODUCTOS.c_departamento, MA_DEPARTAMENTOS.C_DESCRIPCIO
                          UNION
                          SELECT        MONTH(TR_INVENTARIO.f_fecha) AS Mes, MA_PRODUCTOS_3.c_departamento, MA_DEPARTAMENTOS_3.C_DESCRIPCIO, - (1 * SUM((TR_INVENTARIO.n_COSTO * TR_INVENTARIO.n_CANTIDAD) 
                                                   * (MA_VENTAS.n_FACTORCAMBIO / MA_MONEDAS.n_factor))) AS CostoVenta, - (1 * SUM(((TR_INVENTARIO.n_TOTAL - TR_INVENTARIO.n_IMPUESTO) 
                                                   - TR_INVENTARIO.n_COSTO * TR_INVENTARIO.n_CANTIDAD) * (MA_VENTAS.n_FACTORCAMBIO / MA_MONEDAS.n_factor))) AS UTILIDAD, 
                                                   - (1 * SUM((TR_INVENTARIO.n_TOTAL - TR_INVENTARIO.n_IMPUESTO) * (MA_VENTAS.n_FACTORCAMBIO / MA_MONEDAS.n_factor))) AS TotalVentasL, - (1 * SUM(TR_INVENTARIO.n_CANTIDAD)) 
                                                   AS unidadesVenta, 0 AS TOTALCompra, 0 AS UnidadesCompra
                          FROM            TR_INVENTARIO AS TR_INVENTARIO LEFT OUTER JOIN
                                                   MA_VENTAS AS MA_VENTAS ON MA_VENTAS.c_DOCUMENTO = TR_INVENTARIO.c_DOCUMENTO AND MA_VENTAS.c_CONCEPTO = TR_INVENTARIO.c_CONCEPTO LEFT OUTER JOIN
                                                   MA_MONEDAS AS MA_MONEDAS ON MA_MONEDAS.c_codmoneda = '0000000002' LEFT OUTER JOIN
                                                   MA_PRODUCTOS AS MA_PRODUCTOS_3 ON MA_PRODUCTOS_3.C_CODIGO = TR_INVENTARIO.c_CODARTICULO LEFT OUTER JOIN
                                                   MA_DEPARTAMENTOS AS MA_DEPARTAMENTOS_3 ON MA_PRODUCTOS_3.c_departamento = MA_DEPARTAMENTOS_3.C_CODIGO
                          WHERE        (TR_INVENTARIO.c_CONCEPTO = 'DEV') AND (TR_INVENTARIO.n_CANTIDAD &gt; 0) AND (TR_INVENTARIO.f_fecha BETWEEN CAST(CAST(@Ano AS varchar(4)) + '0101' AS DATE) AND 
                                                   CAST(CAST(@Ano AS varchar(4)) + '1231' AS DATE)) AND (TR_INVENTARIO.c_codlocalidad = @Sucursal) AND (MA_PRODUCTOS_3.n_activo IN (@Activo)) AND 
                                                   (MA_PRODUCTOS_3.c_departamento IN (@Departamento))
                          GROUP BY MONTH(TR_INVENTARIO.f_fecha), MA_PRODUCTOS_3.c_departamento, MA_DEPARTAMENTOS_3.C_DESCRIPCIO
                          UNION
                          SELECT        MONTH(TR_INVENTARIO.f_fecha) AS Mes, MA_PRODUCTOS_2.c_departamento, MA_DEPARTAMENTOS_2.C_DESCRIPCIO, 0 AS TotalCostoVenta, 0 AS utilidad, 0 AS totalventa, 0 AS UnidadesVenta, 
                                                   SUM((TR_INVENTARIO.n_COSTO * (CASE TR_INVENTARIO.N_CANT_TEORICA WHEN TR_INVENTARIO.N_CANTIDAD THEN 0 ELSE TR_INVENTARIO.N_CANTIDAD END + TR_INVENTARIO.n_cant_teorica))
                                                    * (MA_COMPRAS.n_FACTORCAMBIO / MA_MONEDAS.n_factor)) AS TOTALCompra, 
                                                   SUM(CASE TR_INVENTARIO.N_CANT_TEORICA WHEN TR_INVENTARIO.N_CANTIDAD THEN 0 ELSE TR_INVENTARIO.N_CANTIDAD END + TR_INVENTARIO.n_cant_teorica) AS UnidadesCompra
                          FROM            TR_INVENTARIO AS TR_INVENTARIO LEFT OUTER JOIN
                                                   MA_MONEDAS AS MA_MONEDAS ON MA_MONEDAS.c_codmoneda = '0000000002' INNER JOIN
                                                   MA_COMPRAS ON MA_COMPRAS.c_DOCUMENTO = TR_INVENTARIO.c_DOCUMENTO AND TR_INVENTARIO.c_CONCEPTO = MA_COMPRAS.C_CONCEPTO LEFT OUTER JOIN
                                                   MA_PRODUCTOS AS MA_PRODUCTOS_2 ON MA_PRODUCTOS_2.C_CODIGO = TR_INVENTARIO.c_CODARTICULO LEFT OUTER JOIN
                                                   MA_DEPARTAMENTOS AS MA_DEPARTAMENTOS_2 ON MA_PRODUCTOS_2.c_departamento = MA_DEPARTAMENTOS_2.C_CODIGO
                          WHERE        (TR_INVENTARIO.c_CONCEPTO = 'COM') AND (TR_INVENTARIO.f_fecha BETWEEN CAST(CAST(@Ano AS varchar(4)) + '0101' AS DATE) AND CAST(CAST(@Ano AS varchar(4)) + '1231' AS DATE)) AND 
                                                   (TR_INVENTARIO.c_codlocalidad = @Sucursal) AND (MA_PRODUCTOS_2.n_activo IN (@Activo)) AND (MA_PRODUCTOS_2.c_departamento IN (@Departamento))
                          GROUP BY MONTH(TR_INVENTARIO.f_fecha), MA_PRODUCTOS_2.c_departamento, MA_DEPARTAMENTOS_2.C_DESCRIPCIO
                          UNION
                          SELECT        MONTH(TR_INVENTARIO.f_fecha) AS Mes, MA_PRODUCTOS_1.c_departamento, MA_DEPARTAMENTOS_1.C_DESCRIPCIO, 0 AS TotalCostoVenta, 0 AS utilidad, 0 AS totalventa, 0 AS UnidadesVenta, 
                                                   - (1 * SUM((TR_INVENTARIO.n_COSTO * (CASE TR_INVENTARIO.N_CANT_TEORICA WHEN TR_INVENTARIO.N_CANTIDAD THEN 0 ELSE TR_INVENTARIO.N_CANTIDAD END + TR_INVENTARIO.n_cant_teorica))
                                                    * (MA_COMPRAS_1.n_FACTORCAMBIO / MA_MONEDAS.n_factor))) AS TOTALCompra, 
                                                   - (1 * SUM(CASE TR_INVENTARIO.N_CANT_TEORICA WHEN TR_INVENTARIO.N_CANTIDAD THEN 0 ELSE TR_INVENTARIO.N_CANTIDAD END + TR_INVENTARIO.n_cant_teorica)) 
                                                   AS UnidadesCompra
                          FROM            TR_INVENTARIO AS TR_INVENTARIO LEFT OUTER JOIN
                                                   MA_MONEDAS AS MA_MONEDAS ON MA_MONEDAS.c_codmoneda = '0000000002' INNER JOIN
                                                   MA_COMPRAS AS MA_COMPRAS_1 ON MA_COMPRAS_1.c_DOCUMENTO = TR_INVENTARIO.c_DOCUMENTO AND TR_INVENTARIO.c_CONCEPTO = MA_COMPRAS_1.C_CONCEPTO LEFT OUTER JOIN
                                                   MA_PRODUCTOS AS MA_PRODUCTOS_1 ON MA_PRODUCTOS_1.C_CODIGO = TR_INVENTARIO.c_CODARTICULO LEFT OUTER JOIN
                                                   MA_DEPARTAMENTOS AS MA_DEPARTAMENTOS_1 ON MA_PRODUCTOS_1.c_departamento = MA_DEPARTAMENTOS_1.C_CODIGO
                          WHERE        (TR_INVENTARIO.c_CONCEPTO = 'DCM') AND (TR_INVENTARIO.f_fecha BETWEEN CAST(CAST(@Ano AS varchar(4)) + '0101' AS DATE) AND CAST(CAST(@Ano AS varchar(4)) + '1231' AS DATE)) AND 
                                                   (TR_INVENTARIO.c_codlocalidad = @Sucursal) AND (MA_PRODUCTOS_1.n_activo IN (@Activo)) AND (MA_PRODUCTOS_1.c_departamento IN (@Departamento))
                          GROUP BY MONTH(TR_INVENTARIO.f_fecha), MA_PRODUCTOS_1.c_departamento, MA_DEPARTAMENTOS_1.C_DESCRIPCIO) AS m
GROUP BY Mes, c_departamento, C_DESCRIPCIO
ORDER BY Mes, departamento</CommandText>
      </Query>
