    <DataSet Name="DataSet_StockFinal">
      <Query>
        <DataSourceName>ExtendVAD10</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Ano">
            <Value>=Parameters!Ano.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Sucursal">
            <Value>=Parameters!Sucursal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Departamento">
            <Value>=Parameters!Departamento.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        MONTH(e.Fecha) AS Mes, e.c_codarticulo, p.C_DESCRI AS producto, p.c_departamento AS codigo_dpto, d.dpto, g.codigo_grupo, g.grupo, s.codigo_subgrupo, s.subgrupo, co.Costo_Actual, co.Costo_Promedio, 
                         SUM(e.Existencia) AS stock, SUM(e.Existencia) * co.Costo_Actual AS Valor_UC, SUM(e.Existencia) * co.Costo_Promedio AS Valor_CP
FROM            HistoricoExistencia AS e INNER JOIN
                         VAD10.dbo.MA_PRODUCTOS AS p ON e.c_codarticulo = p.C_CODIGO INNER JOIN
                             (SELECT        C_CODIGO AS codigo_dpto, C_DESCRIPCIO AS dpto
                               FROM            VAD10.dbo.MA_DEPARTAMENTOS
                               UNION
                               SELECT        ' ' AS codigo_dpto, 'SIN DEPARTAMENTO' AS dpto) AS d ON p.c_departamento = d.codigo_dpto INNER JOIN
                             (SELECT        c_CODIGO AS codigo_grupo, C_DESCRIPCIO AS grupo, c_departamento AS codigo_dpto
                               FROM            VAD10.dbo.MA_GRUPOS
                               UNION
                               SELECT        ' ' AS codigo_grupo, 'SIN GRUPO' AS grupo, ' ' AS codigo_dpto) AS g ON p.c_grupo = g.codigo_grupo LEFT OUTER JOIN
                             (SELECT        c_CODIGO AS codigo_subgrupo, c_DESCRIPCIO AS subgrupo, c_in_grupo AS codigo_grupo, c_in_departamento AS codigo_dpto
                               FROM            VAD10.dbo.MA_SUBGRUPOS
                               UNION
                               SELECT        ' ' AS codigo_subgrupo, 'SIN SUBGRUPO' AS subgrupo, ' ' AS codigo_grupo, ' ' AS codigo_dpto) AS s ON p.c_subgrupo = s.codigo_subgrupo AND p.c_departamento = s.codigo_dpto AND 
                         p.c_grupo = s.codigo_grupo INNER JOIN
                             (SELECT        Fecha, c_codigo, ISNULL(CostoA, 0) AS Costo_Actual, ISNULL(CostoR, 0) AS Costo_Reposicion, ISNULL(CostoPromedio, 0) AS Costo_Promedio
                               FROM            HistoricoCostoPrecio
                               WHERE        (Fecha IN
                                                             (SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0101' AS DATE)) AS Result
                                                               UNION
                                                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0201' AS DATE)) AS Result
                                                               UNION
                                                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0301' AS DATE)) AS Result
                                                               UNION
                                                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0401' AS DATE)) AS Result
                                                               UNION
                                                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0501' AS DATE)) AS Result
                                                               UNION
                                                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0601' AS DATE)) AS Result
                                                               UNION
                                                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0701' AS DATE)) AS Result
                                                               UNION
                                                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0801' AS DATE)) AS Result
                                                               UNION
                                                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0901' AS DATE)) AS Result
                                                               UNION
                                                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '1001' AS DATE)) AS Result
                                                               UNION
                                                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '1101' AS DATE)) AS Result
                                                               UNION
                                                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '1201' AS DATE)) AS Result))) AS co ON p.C_CODIGO = co.c_codigo AND e.Fecha = co.Fecha
WHERE        (e.Fecha IN
                             (SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0101' AS DATE)) AS Result
                               UNION
                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0201' AS DATE)) AS Result
                               UNION
                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0301' AS DATE)) AS Result
                               UNION
                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0401' AS DATE)) AS Result
                               UNION
                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0501' AS DATE)) AS Result
                               UNION
                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0601' AS DATE)) AS Result
                               UNION
                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0701' AS DATE)) AS Result
                               UNION
                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0801' AS DATE)) AS Result
                               UNION
                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '0901' AS DATE)) AS Result
                               UNION
                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '1001' AS DATE)) AS Result
                               UNION
                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '1101' AS DATE)) AS Result
                               UNION
                               SELECT        EOMONTH(CAST(CAST(@Ano AS varchar(4)) + '1201' AS DATE)) AS Result)) AND (e.c_sucursal = @Sucursal) AND (p.c_departamento IN (@Departamento))
GROUP BY e.Fecha, e.c_codarticulo, p.C_DESCRI, p.c_departamento, d.dpto, g.codigo_grupo, g.grupo, s.codigo_subgrupo, s.subgrupo, co.Costo_Actual, co.Costo_Promedio</CommandText>
      </Query>
      <Fields>
        <Field Name="Mes">
          <DataField>Mes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="c_codarticulo">
          <DataField>c_codarticulo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="producto">
          <DataField>producto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="codigo_dpto">
          <DataField>codigo_dpto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dpto">
          <DataField>dpto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="codigo_grupo">
          <DataField>codigo_grupo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="grupo">
          <DataField>grupo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="codigo_subgrupo">
          <DataField>codigo_subgrupo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="subgrupo">
          <DataField>subgrupo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Costo_Actual">
          <DataField>Costo_Actual</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Costo_Promedio">
          <DataField>Costo_Promedio</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="stock">
          <DataField>stock</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Valor_UC">
          <DataField>Valor_UC</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Valor_CP">
          <DataField>Valor_CP</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
