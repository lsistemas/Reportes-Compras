          <DataField>Fecha</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="c_codarticulo">
          <DataField>c_codarticulo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="producto">
          <DataField>producto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="codigo_dpto">
          <DataField>codigo_dpto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dpto">
          <DataField>dpto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="codigo_grupo">
          <DataField>codigo_grupo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="grupo">
          <DataField>grupo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="codigo_subgrupo">
          <DataField>codigo_subgrupo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="subgrupo">
          <DataField>subgrupo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="stock">
          <DataField>stock</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Costo_Actual">
          <DataField>Costo_Actual</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Costo_Promedio">
          <DataField>Costo_Promedio</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Valor_UC">
          <DataField>Valor_UC</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
